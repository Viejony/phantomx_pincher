	<m_link name="${link_05_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="geom_05_name" mass="0.018" in_xyz="0.010769 0 -0.007406" ixx="0.002959" iyy="0.00287" izz="0.004524" ixy="0" ixz="-0.000707" iyz="0" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh="${colli_05_name}"/>
