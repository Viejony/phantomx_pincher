	<m_link name="${link_01_name}" o_rpy="0 0 ${M_PI_M}" o_xyz="0 0 0" geo_mesh="geom_01_name" mass="0.118" in_xyz="0.001031 -0.000032 -0.013806" ixx="0.030893" iyy="0.036432" izz="0.018984" ixy="0.000002" ixz="-0.000383" iyz="-0.000026" co_xyz="0 0 0" co_rpy="0 0 ${M_PI_M}" col_mesh="${colli_01_name}"/>
	<m_joint name="${joint_01_name}" type="revolute" origin_rpy="0 0 0" origin_xyz="0 0 0.137" parent="${link_00_name}" child="${link_01_name}" limit_e="1000.0" limit_l="-2.617993" limit_u="2.617993" limit_v="0.5" />
	<!-- Lower arm -->
	<m_link name="${link_02_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="geom_02_name" mass="0.162" in_xyz="0.080988 -0.000023 -0.000748" ixx="0.031204" iyy="0.124182" izz="0.111578" ixy="0.00002" ixz="0.001864" iyz="-0.000003" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh="${colli_02_name}"/>
	<m_joint name="${joint_02_name}" type="revolute" origin_rpy="${M_PI_M} -${M_PI_M} 0" origin_xyz="0 0 0" parent="${link_01_name}" child="${link_02_name}" limit_e="1000.0" limit_l="-2.617993" limit_u="2.617993" limit_v="0.5" />
	<!-- Upper arm -->
	<m_link name="${link_03_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="geom_03_name" mass="0.162" in_xyz="0.080988 -0.000023 -0.000748" ixx="0.031204" iyy="0.124182" izz="0.111578" ixy="0.00002" ixz="0.001864" iyz="-0.000003" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh="${colli_03_name}"/>
