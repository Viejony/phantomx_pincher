	<!-- Tool -->
	<m_link name="${link_07_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="" mass="0.0001" in_xyz="0 0 0" ixx="0.00001" iyy="0.00001" izz="0.00001" ixy="0.0" ixz="0.0" iyz="0.0" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh=""/>
	<m_joint name="${joint_07_name}" type="fixed" origin_rpy="${M_PI} -${M_PI_M} 0" origin_xyz="0.110 0 0" parent="${link_04_name}" child="${link_07_name}" limit_e="0.0" limit_l="0" limit_u="0.0" limit_v="0.0" />
	<!-- Transmission -->
	<m_trans trans_name="${trans_00_name}" j_name="${joint_01_name}" act_name="${actr_00_name}"/>
	<m_trans trans_name="${trans_01_name}" j_name="${joint_02_name}" act_name="${actr_01_name}"/>
	<m_trans trans_name="${trans_02_name}" j_name="${joint_03_name}" act_name="${actr_02_name}"/>
	<m_trans trans_name="${trans_03_name}" j_name="${joint_04_name}" act_name="${actr_03_name}"/>
	<m_trans trans_name="${trans_04_name}" j_name="${joint_05_name}" act_name="${actr_04_name}"/>
	<m_trans trans_name="${trans_05_name}" j_name="${joint_06_name}" act_name="${actr_05_name}"/>
