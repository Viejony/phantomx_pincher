	<m_link name="${link_00_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="geom_00_name" mass="0.352" in_xyz="-0.027277 -0.000052 0.042715" ixx="0.662213" iyy="0.758616" izz="0.717514" ixy="0.000438" ixz="-0.133928" iyz="-0.000837" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh="${colli_00_name}"/>
