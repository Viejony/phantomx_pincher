	<m_link name="${link_04_name}" o_rpy="0 0 0" o_xyz="0 0 0" geo_mesh="geom_04_name" mass="0.160" in_xyz="0.047054 0.009939 -0.000023" ixx="0.053897" iyy="0.057629" izz="0.068868" ixy="-0.002353" ixz="0.000011" iyz="0.000041" co_xyz="0 0 0" co_rpy="0 0 0" col_mesh="${colli_04_name}"/>
